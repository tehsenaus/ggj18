{
    "sourceMaps": true,
    "presets": [
        ["env", {
            "targets": {
                "browsers": ["last 2 Android versions", "last 2 iOS versions"],
                "node": "8"
            },
            "exclude": ["transform-async-to-generator", "transform-regenerator"],
            "debug": false
        }]
    ],
    "plugins": [
        "lodash",
        "fast-async",
        "transform-object-rest-spread",
        "transform-class-properties",
        ["transform-react-jsx", { "pragma":"h" }]
    ]
}